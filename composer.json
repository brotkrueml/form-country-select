{
    "name": "brotkrueml/form-country-select",
    "description": "TYPO3 form element for selecting a country from a localised list",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/form-country-select/issues",
        "source": "https://github.com/brotkrueml/form-country-select",
        "docs": "https://docs.typo3.org/p/brotkrueml/form-country-select/main/en-us/"
    },
    "require": {
        "php": ">=7.2",
        "symfony/intl": "^4.3 || ^5.0 || ^6.0",
        "typo3/cms-core": "^9.5.16 || ^10.4 || ^11.5",
        "typo3/cms-extbase": "^9.5.16 || ^10.4 || ^11.5",
        "typo3/cms-form": "^9.5.16 || ^10.4 || ^11.5"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~1.2.0",
        "ergebnis/composer-normalize": "~2.18.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "1.2.0",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "rector/rector": "0.12.8",
        "symfony/yaml": "^4.4 || ^5.3 || ^6.0",
        "symplify/phpstan-rules": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\FormCountrySelect\\": "Classes"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.4.x-dev"
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "extension-key": "form_country_select",
            "web-dir": ".Build/web"
        }
    }
}
